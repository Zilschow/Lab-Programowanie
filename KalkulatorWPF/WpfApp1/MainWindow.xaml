        <Button x:Name="butSuma" Content="Suma" HorizontalAlignment="Left" Margin="29,122,0,0" VerticalAlignment="Top" Height="30" Width="118" Click="butSuma_Click"/>
        <Button x:Name="butRoz" Content="Różnica" HorizontalAlignment="Left" Margin="152,122,0,0" VerticalAlignment="Top" Height="30" Width="118" Click="butRoz_Click"/>
        <Button x:Name="butIlocz" Content="Iloczyn" HorizontalAlignment="Left" Margin="29,167,0,0" VerticalAlignment="Top" Height="30" Width="118" Click="butIlocz_Click"/>
        <Button x:Name="butIloraz" Content="Iloraz" HorizontalAlignment="Left" Margin="152,167,0,0" VerticalAlignment="Top" Height="30" Width="118" RenderTransformOrigin="-0.111,1.224" Click="butIloraz_Click"/>
